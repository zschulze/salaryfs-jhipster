relationship OneToMany {
	MsfTeam{msfPlayer} to MsfPlayer{msfTeam(name)},
    MsfTeam{msfGame(awayTeam)} to MsfGame{msfTeam(name)},
    MsfTeam{msfGame(homeTeam)} to MsfGame{msfTeam(name)},
    MsfPlayer{msfPlayerStatsBoxscore} to MsfPlayerStatsBoxscore{msfPlayer},
    MsfGame{msfPlayerStatsBoxscore} to MsfPlayerStatsBoxscore{msfGame}
}

paginate * with pagination
search * with elasticsearch
