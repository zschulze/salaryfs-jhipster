entity MsfTeam {
	city String,
    name String,
    abbreviation String
}

entity MsfGame {
	week Long,
    scheduleStatus String,
    originalDate Instant,
    originalTime Instant,
    delayedOrPostponedReason String,
    actualDate Instant,
    actualTime Instant,
    location String
}

entity MsfPlayer {
	lastName String,
    firstName String,
    jerseyNumber Long,
    position String,
    height String,
    weight Long,
    birthDate Instant,
    age Long,
    birthCity String,
    birthCountry String,
    isRookie Boolean
}

